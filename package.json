{
  "name": "ruleengine.org",
  "version": "1.0.0",
  "description": "Rule Engine Website",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "vuepress dev docs",
    "build": "vuepress build docs",
    "lint": "eslint . --ext .vue,.js --fix --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{js,vue,css,scss}\" --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-angular": "^17.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vuepress/plugin-register-components": "2.0.0-beta.49",
    "eslint": "^8.23.0",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "vuepress": "2.0.0-beta.49"
  },
  "lint-staged": {
    "*.{js,vue,css,scss}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruleenginejs/ruleengine.org.git"
  },
  "author": "Nikolay Naumenkov <nikolas.it.web@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruleenginejs/ruleengine.org/issues"
  },
  "homepage": "https://github.com/ruleenginejs/ruleengine.org#readme"
}
